
#include <stdio.h>
#include <string.h>
#include <math.h>
/* Reads two integers from stdin and prints their sum to stdou
   t. */
int main(void) {
  float x, y;
  /* Get values for the two integers. */
  printf("Enter the value of x: ");
  scanf("%f", &x);
  printf("Enter value the of y: ");
  scanf("%f", &y);
  /* Print the sum. */
  printf("Sum = %f\n", x/y);
  if (x/y > 1){
    printf("nah");
  }
  return 0;
} /* End of main. */
